{
  "name": "vuetable-2",
  "version": "2.0.0-beta.4",
  "description": "Datatable component for Vue 2.x",
  "main": "dist/vuetable-2.js",
  "license": "MIT",
  "author": "Rati Wannapanop <rati.wannapanop@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ratiw/vuetable-2.git"
  },
  "private": false,
  "keywords": [
    "vue",
    "component",
    "table",
    "datatable",
    "pagination-components",
    "json"
  ],
  "browserify": {
    "transform": [
      "babelify",
      [
        "vueify",
        {
          "_flags": {
            "debug": true
          }
        }
      ]
    ]
  },
  "scripts": {
    "dev": "node build/dev-server.js",
    "start": "node build/dev-server.js",
    "build": "node build/build.js && npm run bundle",
    "bundle:min": "cross-env MINIFY=true webpack --progress --config build/webpack.build.config.js",
    "bundle:full": "cross-env MINIFY=false webpack --progress --config build/webpack.build.config.js",
    "bundle": "npm run bundle:min && npm run bundle:full",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^0.15.3"
  },
  "devDependencies": {
    "@vue/test-utils": "1.0.0-beta.10",
    "autoprefixer": "^6.7.2",
    "babel-core": "6.26.3",
    "babel-jest": "22.1.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-istanbul": "4.1.6",
    "babel-plugin-module-resolver": "3.1.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "babelify": "^7.3.0",
    "chalk": "^1.1.3",
    "connect-history-api-fallback": "^1.5.0",
    "copy-webpack-plugin": "4.5.1",
    "cross-env": "^4.0.0",
    "cross-spawn": "^5.0.1",
    "css-loader": "0.28.11",
    "eventsource-polyfill": "^0.9.6",
    "express": "4.16.3",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.11.1",
    "friendly-errors-webpack-plugin": "1.7.0",
    "html-webpack-plugin": "^2.30.1",
    "http-proxy-middleware": "^0.17.3",
    "inject-loader": "^3.0.1",
    "jest": "22.1.4",
    "jest-vue-preprocessor": "1.3.1",
    "json-loader": "^0.5.7",
    "karma": "^1.7.1",
    "karma-coverage": "1.1.2",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.30",
    "karma-webpack": "2.0.13",
    "lolex": "^1.5.2",
    "mocha": "^3.5.3",
    "moxios": "^0.4.0",
    "opn": "^4.0.2",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "ora": "1.4.0",
    "promise-polyfill": "^6.1.0",
    "rimraf": "^2.6.2",
    "semver": "^5.5.0",
    "shelljs": "^0.7.6",
    "stats-webpack-plugin": "0.4.3",
    "url-loader": "^0.5.8",
    "vue": "2.5.16",
    "vue-hot-reload-api": "^1.2.0",
    "vue-html-loader": "1.2.3",
    "vue-loader": "^12.2.2",
    "vue-style-loader": "3.1.2",
    "vue-template-compiler": "2.5.16",
    "vueify": "^9.4.1",
    "webpack": "^2.7.0",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-hot-middleware": "2.22.2",
    "webpack-merge": "4.1.2"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jest": {
    "globals": {
      "VERSION": "dummy4test"
    },
    "moduleNameMapper": {
      "^vue$": "vue/dist/vue.common.js"
    },
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor"
    }
  }
}
