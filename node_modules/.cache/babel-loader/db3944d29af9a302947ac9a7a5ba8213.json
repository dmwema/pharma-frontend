AAI,CAAC2C,KAAT,CAAe;AACvB1C,EAAAA,KAAK,EAAEA,KADgB;AAEvBc,EAAAA,SAAS,EAAEA,SAFY;AAGvBK,EAAAA,OAAO,EAAEA,OAHc;AAIvBe,EAAAA,OAAO,EAAEA,OAJc;AAKvBS,EAAAA,MAAM,EAAE;AALe,CAAf,CAAZ;AAQAC,MAAM,CAAC1B,KAAP,GAAeA,KAAf;AAEA,eAAeA,KAAf","sourcesContent":["import Vuex from 'vuex'\n\nconst state = {\n    config: {\n        apiURL: 'https://pharma-delib-api.herokuapp.com',\n        loading: false\n    },\n\n    fac: {\n        professors: [{\n            id: 1,\n            key: 1,\n            fullname: \"mwema kapwe daniel\",\n            email: \"daniel@mwema.com\",\n            sexe: \"M\"\n        }],\n        courses: [],\n        students: [],\n        selectedProf: {\n            id: 1,\n            key: 1,\n            fullname: \"mwema kapwe daniel\",\n            email: \"daniel@mwema.com\",\n            sexe: \"M\"\n        }\n    }\n}\n\nconst mutations = {\n    ADD_PROF: (state, prof) => {\n        let storeProf = {\n            key: store.getters.profsCount + 1,\n            id: store.getters.profsCount + 1,\n            fullname: prof.lastname + ' ' + prof.middlename + ' ' + prof.firstname,\n            email: prof.email,\n            sexe: prof.sexe\n        }\n        state.fac.professors.push(storeProf)\n    },\n    DELETE_PROF: (state, id) => {\n        let index = state.fac.professors.findIndex(prof => prof.id == id);\n        state.fac.professors.splice(index, 1);\n    },\n    EDIT_PROF: (state, prof) => {\n        const found = state.fac.professors.find(item => item.id === prof.id);\n\n        found.fullname = prof.lastname + ' ' + prof.middlename + ' ' + prof.firstname\n        found.email = prof.email\n        found.sexe = prof.sexe\n    },\n    EDIT_SELECTED_PROF: (state, id) => {\n        state.fac.selectedProf = state.fac.professors.find(prof => prof.id === id)\n    }\n}\n\nconst actions = {\n    addProf: (store, prof) => {\n        store.commit('ADD_PROF', prof)\n    },\n    editProf: (store, prof) => {\n        store.commit('EDIT_PROF', prof)\n    },\n    deleteProf: (store, id) => {\n        store.commit('DELETE_PROF', id)\n    },\n    editSelectedProf: (store, id) => {\n        store.commit('EDIT_SELECTED_PROF', id)\n    }\n}\n\nconst getters = {\n    profs: state => state.fac.professors,\n    selectedProf: state => state.fac.selectedProf,\n    profsCount: state => state.fac.professors.length,\n}\n\nlet store = new Vuex.Store({\n    state: state,\n    mutations: mutations,\n    getters: getters,\n    actions: actions,\n    strict: true\n})\n\nglobal.store = store\n\nexport default store"]}]}